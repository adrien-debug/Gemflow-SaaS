    <changeSet id="003_CURRENCY_UPDATE" author="${author}">
        <addColumn tableName="currency">
            <column name="is_active" type="boolean"/>
        </addColumn>
        <update tableName="currency">
            <column name="is_active" value="false"/>
        </update>
        <update tableName="currency">
            <column name="is_active" value="true"/>
            <where>code IN ('AED', 'USD', 'EUR', 'GBP')</where>
        </update>
        <addNotNullConstraint tableName="currency" columnName="is_active"/>
        <addNotNullConstraint tableName="currency" columnName="decimal_digits"/>
        <addNotNullConstraint tableName="currency" columnName="rounding"/>
        <addNotNullConstraint tableName="currency" columnName="name_plural"/>
    </changeSet>
