{
  "name": "atelier-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:stage": "tsc -b && vite build --mode stage",
    "build:test": "tsc -b && vite build --mode test",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "preview": "vite preview",
    "deploy:stage": "aws s3 sync ./dist s3://atelier-frontend-stage --delete",
    "deploy:test": "aws s3 sync ./dist s3://atelier-frontend-test --delete"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.2",
    "@tanstack/react-query": "^5.62.10",
    "antd": "^5.26.2",
    "axios": "^1.8.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.0.2",
    "react-timer-hook": "^4.0.5",
    "react-to-print": "^3.1.1",
    "tui-image-editor": "^3.15.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "prettier": "^3.4.2",
    "sass-embedded": "^1.83.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
